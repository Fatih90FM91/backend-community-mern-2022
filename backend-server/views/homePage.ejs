<%- include('./components/header') %>

<section class="section-homepage">

    
<div>
    <a class="btn btn-danger" style="color: #FF7800;" href="/showQuestion" >Back Questions!</a>
  <% if(answers.length){ %>
    <% answers.forEach( answer => { %>
        <div class="card homepage-answer-1"  style="width: 70%;">
            <!-- <div class="card-header answer-header"  style="width: 25%;"> -->
           <p> This Answer is from : <span class="username-hmp"><a href="#"><%= answer.user_id.username %> </a></span></p>    
            <!-- </div> -->
            <div class="card-body answer-img">
                <h5 class="card-title"><%= answer.answer %></h5>
                <img src="data:image/png;base64," alt="logo" width="15%">
            </div>
        </div>
        <p class="date-of-answer"><%= answer.created_at%></p>
        
    <% }) %>
<% } else { %>
    <p class="card card-header answer-header card-body">There is No answer</p>
<% } %>
</div>
<div class="imgs-right-side">
    <img class="ing-img-hmp" src="https://previews.123rf.com/images/visualizestudio/visualizestudio1704/visualizestudio170400395/76979460-atm-bankautomaat-of-ing-bank-in-antwerp.jpg" alt="logo" width="50%">
    <br><br><br><br>
    <img src="https://media-exp1.licdn.com/dms/image/C511BAQFQmAIWm9Edag/company-background_10000/0/1585726889443?e=2159024400&v=beta&t=hURV-EK8dc0jcFgfTAqHgTvuS2hDtfJdCIIA897VSuE" alt="logo" width="50%">
</div>
</section>
<%- include('./components/footer') %>