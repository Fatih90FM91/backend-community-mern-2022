
<%- include('../components/header') %>


<% if(questions.length!= 0){ %>
    <% questions.forEach( question => { %>
        <p class="date-of-question"><%= question.created_at%></p>
        <div class="showQuestion card w-75">
            <div class="images-questions">
                <img src="https://www.ing.com/upload_mm/9/c/2/52293_fullimage_find-ing-in-your-area-250x250px.png" alt="logo" width="10%" class="question-logo">
                  <img width="10%" src="data:image/png;base64," alt="">
            </div>
            
        <div class="card-header">
            This Answer is from : <span><%= question.user_id.username %></span>
        </div>
      
<div class="question">

<h2><%= question.question %></h2>
<p><%= question.description %></p>

<a href="/show-question/<%= question.id %>">See More </a> 
</div>
</div>
        <%    } )%>
        <%    } %>



        <%- include('../components/footer') %>